{
  "name": "jacq-portfolio-2020",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon app.js && cd client && node app.js",
    "dev": "concurrently \"nodemon app.js\" \"cd client && npm start \"",
    "heroku-prebuild": "rm -rf public/ && mkdir public",
    "heroku-postbuild": "cd client && npm i && npm run build"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "concurrently": "^5.0.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "hbs": "^4.1.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
